        {
            "dimensions": {
                "boxWidth": "${viewport.width/4}",
                "boxHeight": 1000
            }
        }
    ],
    "mainTemplate": {
        "parameters": [
            "payload"
        ],
        "items": [
            {
                "type": "Container",
                "width": "100vw",
                "height": "100vh",
                "justifyContent": "start",
                "alignItems": "center",
                "bind": [
                    {
                        "name": "dark_uno",
                        "value": 45
                    },
                    {
                        "name": "dark_dos",
                        "value": 45
                    },
                    {
                        "name": "dark_tres",
                        "value": 45
                    },
                    {
                        "name": "dark_cuatro",
                        "value": 45
                    },
                    {
                        "name": "dark_cinco",
