                        "height": "50vh",
                        "justifyContent": "center",
                        "alignItems": "center",
                        "direction": "row",
                        "items": [
                            {
                                "type": "TouchWrapper",
                                "id":"touch_uno",
                                "speech": "${payload.itemData.properties.unoSpeech.url}",
                                "items": [{
                                    "type": "Frame",
                                    "id": "id_uno",
                                    "backgroundColor": "hsl(118, 100%, ${dark_uno}%)",
                                    "width": "@boxWidth",
                                    "height": "@boxHeight",
                                    "description": "frame1-VERDE"
                                }
                                ],
                                "onPress": [
                                    {
                                        "type": "Parallel",
                                        "commands": [
                                     
                                            {
                                                "type": "AnimateItem",
                                                "componentId": "id_uno",
                                                "duration": 150,
                                                "repeatCount": 1,
                                                "repeatMode": "reverse",
                                                "value": {
                                                    "property": "dark_uno",
                                                    "to": "90"
                                                }
                                            },
                                            {
                                                "type": "SendEvent",
                                                "arguments": [
                                                    "verde"
                                                ]
                                            }
                                        ]
                                    }
                                ]
                            },
                            {
                                "type": "TouchWrapper",
                                 "id":"touch_dos",
